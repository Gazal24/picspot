<h1>New picture</h1>

<% form_for(@picture, :html => { :multipart => true }) do |f| %>
<%= f.error_messages %>

<p>
  <%= f.label :album_id %><br />
  <%= f.text_field :album_id %>
</p>
<p>
  <%= f.label :user_id %><br />
  <%= f.text_field :user_id %>
</p>
<p>
  <%= f.label :photo %><br />
  <%= f.file_field :photo %>
</p>
<p>
  <%= f.submit "Create" %>
</p>
<% end %>

<%= link_to 'Back', pictures_path %>
